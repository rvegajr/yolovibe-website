{
  "name": "astroship",
  "type": "module",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "health-check": "npx tsx cli/health-check.ts",
    "health-check:json": "npx tsx cli/health-check.ts --json",
    "health-check:verbose": "npx tsx cli/health-check.ts --verbose",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint \"src/**/*.ts\" \"cli/**/*.ts\" --ext .ts",
    "lint:fix": "eslint \"src/**/*.ts\" \"cli/**/*.ts\" --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"cli/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"cli/**/*.ts\"",
    "type-check": "tsc --noEmit --project tsconfig.cli.json",
    "validate": "npm run type-check && npm run lint",
    "validate:fix": "npm run type-check && npm run lint:fix && npm run format",
    "deploy:staging": "vercel --target preview",
    "deploy:production": "vercel --prod",
    "email:test": "npx tsx cli/send-test-email.ts",
    "email:workshop": "npx tsx cli/send-test-email.ts --type workshop",
    "validate:integrations": "npx tsx cli/validate-integrations.ts",
    "test:interfaces": "npx tsx src/registration/cli/test-all-interfaces.ts",
    "test:product-catalog": "npx tsx src/registration/cli/test-product-catalog.ts",
    "test:booking-manager": "npx tsx src/registration/cli/test-booking-manager.ts",
    "test:payment-processor": "npx tsx src/registration/cli/test-payment-processor.ts",
    "test:purchase-manager": "npx tsx src/registration/cli/test-purchase-manager.ts",
    "test:purchase-implementation": "npx tsx src/registration/cli/test-purchase-implementation.ts",
    "test:purchase-api": "npx tsx src/registration/cli/test-purchase-api.ts",
    "test:purchase-emails": "npx tsx src/registration/cli/test-purchase-email-notifications.ts",
    "test:complete-email-workflow": "npx tsx src/registration/cli/test-complete-email-workflow.ts",
    "test:attendee-manager": "npx tsx src/registration/cli/test-attendee-manager.ts",
    "test:email-sender": "npx tsx src/registration/cli/test-email-sender.ts",
    "test:point-of-contact-manager": "npx tsx src/registration/cli/test-point-of-contact-manager.ts",
    "test:attendee-access-manager": "npx tsx src/registration/cli/test-attendee-access-manager.ts",
    "test:coupon-manager": "tsx src/registration/cli/test-coupon-manager.ts",
    "test:workshop-admin": "tsx src/registration/cli/test-workshop-admin.ts",
    "test:calendar-manager": "tsx src/registration/cli/test-calendar-manager.ts",
    "test:user-authenticator": "tsx src/registration/cli/test-user-authenticator.ts",
    "test:material-manager": "tsx src/registration/cli/test-material-manager.ts",
    "test:reporting-manager": "tsx src/registration/cli/test-reporting-manager.ts",
    "test:api": "npx tsx cli/test-api-integration.ts"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.2.0",
    "@astrojs/sitemap": "^3.2.1",
    "@fontsource-variable/bricolage-grotesque": "^5.2.5",
    "@fontsource-variable/inter": "^5.2.5",
    "@fontsource-variable/playfair-display": "^5.2.6",
    "@fontsource-variable/space-grotesk": "^5.2.8",
    "@iconify-json/bx": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.28",
    "@iconify-json/tabler": "^1.2.19",
    "@iconify-json/uil": "^1.2.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.14",
    "@types/bcrypt": "^5.0.2",
    "astro": "^5.9.2",
    "astro-icon": "^1.1.5",
    "astro-navbar": "^2.3.9",
    "astro-seo": "^0.8.4",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.10.0",
    "sharp": "^0.33.5",
    "square": "^42.3.0",
    "tailwindcss": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-v8": "^3.2.4",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "googleapis": "^150.0.1",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
