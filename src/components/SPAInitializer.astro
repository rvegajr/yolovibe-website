          const workshopSelect = document.querySelector('select[name="productId"]');
          if (workshopSelect) {
            workshopSelect.value = selectedWorkshop;
            workshopSelect.dispatchEvent(new Event('change'));
          }
        }, 500);
      }
      
      // Emit initialization complete event
      globalEventBus.emit(EventTypes.USER_INTERACTION, {
        action: 'view',
        element: 'spa-application',
        page: window.location.pathname,
        timestamp: new Date(),
        metadata: {
          initialized: true,
          version: '1.0.0',
          features: ['workshop-selection', 'navigation-enhancement', 'booking-integration']
        }
      });
      
      console.log('✨ YOLOVibe SPA initialization complete! Event-driven features are active.');
      
      // Dispatch custom event for other scripts
      document.dispatchEvent(new CustomEvent('spa-initialized', {
        detail: {
          timestamp: new Date(),
          version: '1.0.0',
          globalEventBus: globalEventBus
        }
      }));
      
    } catch (error) {
      console.error('❌ SPA initialization failed:', error);
      
      globalEventBus.emit(EventTypes.ERROR_OCCURRED, {
        error: error.message,
        page: window.location.pathname,
        action: 'initialize',
        severity: 'critical',
        timestamp: new Date()
      });
    }
  }
  
  // Wait for DOM and initialize
  function waitForDOM() {
    return new Promise((resolve) => {
      if (document.readyState === 'loading') {
        document.addEventListener('DOMContentLoaded', resolve);
      } else {
        resolve();
      }
    });
  }
  
  // Auto-initialize (but not during tests)
  waitForDOM().then(() => {
    // Skip SPA initialization during Playwright tests
    if (window.navigator.webdriver || window.location.search.includes('playwright')) {
      console.log('🧪 Skipping SPA initialization during tests');
      return;
    }
    
    // Small delay to ensure all components are loaded
    setTimeout(initializeSPA, 100);
  });
</script> 