// SPA Initializer Component - Handles the transformation to SPA
---

<script>
  // 🚀 SPA INITIALIZATION - Transforms the site into an event-driven SPA
  
  // Global Event Bus Implementation (Simplified for client-side)
  class GlobalEventBus {
    constructor() {
      this.listeners = new Map();
      this.eventHistory = [];
      this.options = {
        enableHistory: true,
        enableCrossTabs: false,
        enableDebug: true,
        maxHistorySize: 100
      };
      
      console.log('🎉 GlobalEventBus initialized');
    }
    
    emit(eventType, payload) {
      const event = {
        type: eventType,
        payload,
        timestamp: new Date(),
        id: Math.random().toString(36).substr(2, 9)
      };
      
      if (this.options.enableHistory) {
        this.eventHistory.push(event);
        if (this.eventHistory.length > this.options.maxHistorySize) {
          this.eventHistory.shift();
        }
      }
      
      if (this.options.enableDebug) {
        console.log(`🎯 Event: ${eventType}`, payload);
      }
      
      const eventListeners = this.listeners.get(eventType);
      if (eventListeners) {
        eventListeners.forEach(listener => {
          try {
            listener(payload);
          } catch (error) {
            console.error(`❌ Error in event listener for ${eventType}:`, error);
          }
        });
      }
    }
    
    on(eventType, listener) {
      if (!this.listeners.has(eventType)) {
        this.listeners.set(eventType, new Set());
      }
      this.listeners.get(eventType).add(listener);
      
      return () => this.off(eventType, listener);
    }
    
    off(eventType, listener) {
      const eventListeners = this.listeners.get(eventType);
      if (eventListeners) {
        eventListeners.delete(listener);
      }
    }
    
    navigateTo(path, data = {}) {
      this.emit('PAGE_NAVIGATION', {
        from: window.location.pathname,
        to: path,
        data,
        navigationMethod: 'push',
        timestamp: new Date()
      });
      
      // Use browser's history API for now (can be enhanced later)
      if (path !== window.location.pathname) {
        window.history.pushState(data, '', path);
        
        // Emit a custom navigation event
        window.dispatchEvent(new CustomEvent('spa-navigate', {
          detail: { path, data }
        }));
      }
    }
    
    selectWorkshop(workshopId, workshopData) {
      this.emit('WORKSHOP_SELECTED', {
        workshopId,
        workshopName: workshopData.workshopName,
        price: workshopData.price,
        duration: workshopData.duration,
        timestamp: new Date()
      });
      
      // Navigate to booking page with workshop data
      this.navigateTo('/book', { selectedWorkshop: workshopId, workshopData });
    }
  }
  
  // Event Types
  const EventTypes = {
    WORKSHOP_SELECTED: 'WORKSHOP_SELECTED',
    DATE_SELECTED: 'DATE_SELECTED', 
    FORM_SUBMITTED: 'FORM_SUBMITTED',
    USER_INTERACTION: 'USER_INTERACTION',
    ERROR_OCCURRED: 'ERROR_OCCURRED',
    PAGE_NAVIGATION: 'PAGE_NAVIGATION',
    BOOKING_SUBMITTED: 'BOOKING_SUBMITTED'
  };
  
  // Initialize global event bus
  const globalEventBus = new GlobalEventBus();
  
  // Make it available globally for debugging and other scripts
  window.globalEventBus = globalEventBus;
  window.EventTypes = EventTypes;
  
  // SPA Enhancement Functions
  function enhanceWorkshopSelection() {
    // Find all workshop selection buttons
    const workshopButtons = document.querySelectorAll('[data-workshop-id]');
    
    workshopButtons.forEach(button => {
      const buttonText = button.textContent?.trim();
      
      if (buttonText === 'Learn More') {
        // Handle "Learn More" buttons differently
        button.addEventListener('click', (event) => {
          event.preventDefault();
          
          const workshopId = button.getAttribute('data-workshop-id');
          const workshopName = button.getAttribute('data-workshop-name') || 'Workshop';
          const price = parseInt(button.getAttribute('data-workshop-price') || '0');
          const duration = parseInt(button.getAttribute('data-workshop-duration') || '3');
          
          // Show detailed workshop information modal
          showWorkshopDetails(workshopId, workshopName, price, duration);
        });
      } else {
        // Handle workshop selection buttons
        button.addEventListener('click', (event) => {
          event.preventDefault();
          
          const workshopId = button.getAttribute('data-workshop-id');
          const workshopName = button.getAttribute('data-workshop-name') || 'Workshop';
          const price = parseInt(button.getAttribute('data-workshop-price') || '0');
          const duration = parseInt(button.getAttribute('data-workshop-duration') || '3');
          
          const workshopData = {
            workshopName,
            price,
            duration
          };
          
          // Show notification
          showNotification(`✅ ${workshopName} selected!`, 'success', `Price: $${price.toLocaleString()} • Next: Choose your date`);
          
          // Select workshop via event bus
          globalEventBus.selectWorkshop(workshopId, workshopData);
        });
      }
    });
    
    console.log(`🎯 Enhanced ${workshopButtons.length} workshop selection buttons`);
  }
  
  function enhanceNavigation() {
    // Find all internal navigation links
    const navLinks = document.querySelectorAll('a[href^="/"], a[href^="#"]');
    
    navLinks.forEach(link => {
      // Skip external links and those already processed
      if (link.hasAttribute('data-external') || link.hasAttribute('data-spa-enhanced')) {
        return;
      }
      
      link.setAttribute('data-spa-enhanced', 'true');
      
      link.addEventListener('click', (event) => {
        const href = link.getAttribute('href');
        
        if (href.startsWith('#')) {
          // Handle hash navigation (smooth scroll)
          const element = document.getElementById(href.substring(1));
          if (element) {
            event.preventDefault();
            element.scrollIntoView({ behavior: 'smooth' });
          }
        } else if (href.startsWith('/')) {
          // For now, let normal navigation work but emit events
          globalEventBus.emit(EventTypes.USER_INTERACTION, {
            action: 'navigate',
            element: 'nav-link',
            page: window.location.pathname,
            target: href,
            timestamp: new Date()
          });
        }
      });
    });
    
    console.log(`🔗 Enhanced ${navLinks.length} navigation links`);
  }
  
